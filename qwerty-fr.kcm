}

key 8 {
    label:                      '8'
    base:                       '8'
    shift, capslock:            '*'
    ralt:                       '\u00ee'
    ralt+shift, ralt+capslock:  '\u00ce'
}

key 9 {
    label:                      '9'
    base:                       '9'
    shift, capslock:            '('
    ralt:                       '\u00f4'
    ralt+shift, ralt+capslock:  '\u00d4'
}

key 0 {
    label:                      '0'
    base:                       '0'
    shift, capslock:            ')'
    ralt:                       '\u00f8'
    ralt+shift, ralt+capslock:  '\u00d8'
}

key MINUS {
    label:                      '-'
    base:                       '-'
    shift, capslock:            '_'
