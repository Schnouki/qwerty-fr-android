#
# qwerty-fr keyboard layout.
#

type OVERLAY

map key 86 PLUS

### ROW 1

key GRAVE {
    label:                      '`'
    base:                       '`'
    shift, capslock:            '~'
    ralt:                       '\u0300'
    ralt+shift, ralt+capslock:  '\u0303'
}

key 1 {
    label:                      '1'
    base:                       '1'
    shift, capslock:            '!'
    ralt:                       '\u00b9'
    ralt+shift, ralt+capslock:  '\u00a1'
}

key 2 {
    label:                      '2'
    base:                       '2'
    shift, capslock:            '@'
    ralt:                       '\u00b2'
    ralt+shift, ralt+capslock:  none
}

key 3 {
    label:                      '3'
    base:                       '3'
    shift, capslock:            '#'
    ralt:                       '\u00ea'
    ralt+shift, ralt+capslock:  '\u00ca'
}

key 4 {
    label:                      '4'
    base:                       '4'
    shift, capslock:            '$'
    ralt:                       '\u20ac'
    ralt+shift, ralt+capslock:  none
}

key 5 {
    label:                      '5'
    base:                       '5'
    shift, capslock:            '%'
    ralt:                       none
    ralt+shift, ralt+capslock:  none
}

key 6 {
    label:                      '6'
    base:                       '6'
